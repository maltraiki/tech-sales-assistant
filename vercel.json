{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/server.js"
    }
  ],
  "functions": {
    "dist/server.js": {
      "maxDuration": 30,
      "includeFiles": "public/**"
    }
  }
}